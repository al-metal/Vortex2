<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    app:layout_behavior="@string/appbar_scrolling_view_behavior"
    tools:context="com.vortex.vortex.ActivityAPKDezinfekciyaTuman2"
    tools:showIn="@layout/app_bar_activity_apkdezinfekciya_tuman2">

    <ScrollView style="@style/scrollView">

        <LinearLayout style="@style/linearLayoutInputData">

            <TableLayout style="@style/tableViewInputData">

                <TableRow style="@style/tableRow">

                    <ImageButton
                        android:id="@+id/imageButton32"
                        style="@style/imageButtonHorizontal"
                        android:onClick="onClickTumanFire"
                        app:srcCompat="@drawable/vortex_menu_profilaktika_tuman_fire" />
                </TableRow>

                <TableRow style="@style/tableRow">

                    <ImageButton
                        android:id="@+id/imageButton33"
                        style="@style/imageButtonHorizontal"
                        android:onClick="onClickTumanIce"
                        app:srcCompat="@drawable/vortex_menu_profilaktika_tuman_ice" />
                </TableRow>

                <TableRow style="@style/tableRow">

                    <ImageButton
                        android:id="@+id/imageButton34"
                        style="@style/imageButtonHorizontal"
                        android:onClick="onClickTumanIceAnimal"
                        app:srcCompat="@drawable/vortex_menu_profilaktika_tuman_animal" />
                </TableRow>

            </TableLayout>

        </LinearLayout>

    </ScrollView>

</android.support.constraint.ConstraintLayout>
